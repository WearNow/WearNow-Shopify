{% comment %}
  <!-- Button to open the first modal -->
  <button onclick="openModal()">Open Modal</button>

  <script>
    var selectedData = {};
    function openModal() {
      document.getElementById('myModal').style.display = 'block';
    }
    function closeModal() {
      selectedData.gender = document.getElementById('gender').value;
      selectedData.height = document.getElementById('height').value;
      selectedData.weight = document.getElementById('weight').value;
      selectedData.size = document.getElementById('size').value;
      selectedData.fit = document.getElementById('fit').value;
      document.getElementById('myModal').style.display = 'none';
    }
    function openModal2() {
      document.getElementById('myModal2').style.display = 'block';
    }

    function closeModal2() {
      selectedData.shoulders = document.getElementById('shoulders').value;
      selectedData.waist = document.getElementById('waist').value;
      selectedData.hips = document.getElementById('hips').value;
      document.getElementById('myModal2').style.display = 'none';
    }

    function openModal3() {
      document.getElementById('myModal3').style.display = 'block';
      // Set a timeout to close the third modal and open the fourth after 5 seconds
      setTimeout(function () {
        closeModal3();
        openModal4();
        displaySelectedData(); // Display the data in the fourth modal
      }, 3000);
    }

    function back_btn(){
      document.getElementById('myModal').style.display = 'block';
      document.getElementById('myModal2').style.display = 'none';
    }

    function closeModal3() {
      document.getElementById('myModal3').style.display = 'none';
    }

    function openModal4() {
      document.getElementById('myModal4').style.display = 'block';
    }

    function closeModal4() {
      document.getElementById('myModal4').style.display = 'none';
    }
    function displaySelectedData() {
      var resultsContainer = document.getElementById('sizeResults');
      resultsContainer.innerHTML =
        '<p>Gender: <span>' +
        selectedData.gender +
        '</span> </p>' +
        '<p>Height: <span> ' +
        selectedData.height +
        '</span></p>' +
        '<p>Weight: <span> ' +
        selectedData.weight +
        '</span></p>' +
        '<p>Size: <span> ' +
        selectedData.size +
        '</span></p>' +
        '<p>Fit: <span> ' +
        selectedData.fit +
        '</span></p>' +
        '<p>Shoulders: <span> ' +
        selectedData.shoulders +
        '</span></p>' +
        '<p>Waist: <span> ' +
        selectedData.waist +
        '</span></p>' +
        '<p>Hips: <span> ' +
        selectedData.hips +
        '</span></p>';
    }
  </script>
